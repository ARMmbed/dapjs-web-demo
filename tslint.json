{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "only-arrow-functions": [true],
        "promise-function-async": true,
        "no-console": [false],
        "no-bitwise": false,
        "object-literal-sort-keys": false,
        "object-literal-key-quotes": [true, "as-needed"],
        "prefer-for-of": false
    },
    "rulesDirectory": []
}